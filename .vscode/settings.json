{
  "java.configuration.updateBuildConfiguration": "disabled",
  "java.import.gradle.enabled": false,
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "bin/": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true,
    "**/*~": true
  },
  "C_Cpp.default.configurationProvider": "vscode-wpilib",
  "files.associations": {
    "any": "cpp",
    "bit": "cpp",
    "codecvt": "cpp",
    "compare": "cpp",
    "condition_variable": "cpp",
    "cstddef": "cpp",
    "forward_list": "cpp",
    "list": "cpp",
    "string": "cpp",
    "optional": "cpp",
    "future": "cpp",
    "limits": "cpp",
    "memory": "cpp",
    "mutex": "cpp",
    "new": "cpp",
    "functional": "cpp",
    "regex": "cpp",
    "iostream": "cpp"
  }
}
